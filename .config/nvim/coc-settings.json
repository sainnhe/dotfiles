{
    "$schema": "/home/sainnhe/.local/share/nvim/plugins/coc.nvim/data/schema.json",
    // "http.proxy": "http://127.0.0.1:1081",
    // "http.proxyStrictSSL": "http://127.0.0.1:1081",
    "suggest.timeout": 500,
    "suggest.triggerCompletionWait": 0,
    "suggest.enablePreview": true,
    "suggest.maxPreviewWidth": 35,
    "suggest.detailMaxLength": 100,
    "suggest.detailField": "preview",
    "suggest.removeDuplicateItems": true,
    "suggest.snippetIndicator": " \ue796",
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },
    "diagnostic.level": "hint",
    "diagnostic.refreshOnInsertMode": false,
    "diagnostic.refreshAfterSave": true,
    "diagnostic.displayByAle": true,
    "diagnostic.virtualText": true,
    "diagnostic.errorSign": "\uf65b",
    "diagnostic.warningSign": "\uf421",
    "diagnostic.infoSign": "ℹ",
    "diagnostic.hintSign": "➤",
    "codeLens.enable": true,
    "codeLens.separator": "‣",
    "list.indicator": "➤",
    "list.maxHeight": 15,
    "list.insertMappings": {
        "<C-c>": "do:exit",
        "<C-p>": "action:preview",
        "<C-j>": "do:next",
        "<C-k>": "do:previous",
        "<C-s>": "do:toggle",
        "<C-f>": "do:switch",
        "<A-x>": "do:toggleMode",
        "<Down>": "prompt:next",
        "<Up>": "prompt:previous",
        "<C-v>": "prompt:paste"
    },
    "list.normalMappings": {
        "<C-c>": "do:exit"
    },
    "git.changedSign.text": "▌",
    "git.addedSign.text": "▌",
    "git.removedSign.text": "▁",
    "git.changeRemovedSign.text": "▁",
    "git.topRemovedSign.text": "▔",
    "git.changedDecorator": " ",
    "git.stagedDecorator": " ",
    "git.untrackedDecorator": " ✩",
    "git.conflictedDecorator": "⇡⇣",
    "git.addGBlameToVirtualText": true,
    "git.virtualTextPrefix": " \ue729 ",
    "project.dbpath": "~/.cache/vim/coc-project",
    "project.rootPatterns": [".git", ".svn", ".root", ".project", ".hg"],
    "bookmark.sign": "\uf5c2",
    "suggest.languageSourcePriority": 99,
    "snippets.shortcut": "S",
    "snippets.priority": 98,
    "snippets.extends": {
        "cpp": ["c"],
        "javascriptreact": ["javascript"],
        "typescript": ["javascript"]
    },
    "snippets.userSnippetsDirectory": "~/.local/share/nvim/snippets",
    "nextword.filetypes": ["markdown", "text", "help", "gitcommit", "snippets"],
    "coc.source.file.shortcut": "F",
    "coc.source.file.priority": 89,
    "coc.source.file.ignoreHidden": false,
    "coc.source.around.shortcut": "A",
    "coc.source.around.priority": 88,
    "coc.source.buffer.shortcut": "B",
    "coc.source.buffer.priority": 87,
    "coc.source.buffer.ignoreGitignore": false,
    "coc.source.syntax.shortcut": "X",
    "coc.source.syntax.priority": 79,
    "coc.source.tag.shortcut": "T",
    "coc.source.tag.priority": 78,
    "coc.source.dictionary.shortcut": "D",
    "coc.source.dictionary.priority": 9,
    "coc.source.dictionary.filetypes": ["markdown", "text", "help", "gitcommit", "snippets"],
    "coc.source.emoji.shortcut": "E",
    "coc.source.emoji.priority": 8,
    "coc.source.emoji.filetypes": ["markdown", "text"],
    "coc.preferences.colorSupport": true,
    "highlight.document.enable": true,
    "highlight.colors.enable": false,
    "highlight.disableLanguages": [],
    "emmet.includeLanguages": {
        "vue-html": "html",
        "javascript": "javascriptreact"
    },
    "pairs.enableCharacters": ["(", "[", "{", "'", "\"", "`"],
    "explorer.icon.enableNerdfont": true,
    "explorer.keyMappingMode": "none",
    "explorer.previewAction.onHover": false,
    "explorer.keyMappings": {
        "s": "toggleSelection",
        ":": "actionMenu",

        "h": "collapse",
        "l": ["expandable?", "expand", "open"],
        "L": "expandRecursive",
        "H": "collapseRecursive",
        "o": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
        "<cr>": "open",
        "t": "open:tab",
        "<bs>": "gotoParent",

        "Y": "copyFilepath",
        "y": "copyFilename",
        "c": "copyFile",
        "x": "cutFile",
        "p": "pasteFile",
        "d": "delete",
        "D": "deleteForever",

        "n": "addFile",
        "N": "addDirectory",
        "r": "rename",

        ".": "toggleHidden",
        "R": "refresh",
        "<C-p>": "preview",

        "?": "help",
        "q": "quit",
        "<esc>": "esc",
        "e": "systemExecute",

        "f": "searchRecursive",

        "gf": "gotoSource:file",
        "gb": "gotoSource:buffer",

        "gk": "gitPrev",
        "gj": "gitNext",
        "ga": "gitStage",
        "gu": "gitUnstage"
    },
    "clangd.enabled": true,
    "clangd.arguments": ["--clang-tidy", "--completion-style=detailed", "--fallback-style=google", "--header-insertion=iwyu", "--suggest-missing-includes"],
    "clangd.semanticHighlighting": true,
    "rust-analyzer.serverPath": "rust-analyzer",
    "rust-analyzer.updates.channel": "nightly",
    "markdownlint.config": {
        "MD013": false,
        "MD041": false
    },
    "languageserver": {
        "ccls": {
            "enable": false,
            "command": "ccls",
            "filetypes": ["c", "cpp", "objc", "objcpp"],
            "rootPatterns": [".ccls", "compile_commands.json", ".vim/", ".git/", ".hg/"],
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/ccls"
                },
                "highlight": {
                    "lsRanges": true
                }
            }
        },
        "pylance": {
            "enable": true,
            "filetypes": ["python"],
            "command": "pylance",
            "args": ["--stdio"],
            "initializationOptions": {},
            "settings": {
                "python.analysis.typeCheckingMode": "basic",
                "python.analysis.diagnosticMode": "openFilesOnly",
                "python.analysis.stubPath": "./typings",
                "python.analysis.autoSearchPaths": true,
                "python.analysis.extraPaths": [],
                "python.analysis.diagnosticSeverityOverrides": {},
                "python.analysis.useLibraryCodeForTypes": true,
                "python.analysis.autoImportCompletions": true,
                "python.analysis.completeFunctionParens": false
            }
        }
    }
}
