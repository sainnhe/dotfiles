        "--hidden",
        "--no-ignore"
    ],
    "list.insertMappings": {
        "<C-p>": "action:preview",
        "<C-j>": "do:next",
        "<C-k>": "do:previous",
        "<C-s>": "do:toggle",
        "<C-f>": "do:switch",
        "<A-x>": "do:togglemode",
        "<Down>": "prompt:next",
        "<Up>": "prompt:previous",
        "<C-v>": "prompt:paste"
    },
    "coc.preferences.enableLinkedEditing": true,
    "coc.preferences.enableMessageDialog": true,
    "coc.preferences.formatOnType": true,
    "links.tooltip": true,
    "git.changedSign.text": "▌",
